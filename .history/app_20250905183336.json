{
  "expo": {
    "name": "RevoFit",
    "slug": "revofit",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "revofit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.emplica.revofit",
      "appleTeamId": "NAN7MFNMT7",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "NSHealthShareUsageDescription": "RevoFit a besoin d'accéder à vos données de santé pour synchroniser vos entraînements et améliorer votre expérience fitness.",
        "NSHealthUpdateUsageDescription": "RevoFit souhaite enregistrer vos données d'entraînement dans Apple Health pour un suivi complet de votre activité physique."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emplica.revofit",
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "permissions": [
              "com.android.vending.BILLING"
            ]
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification_icon.png",
          "color": "#ffffff",
          "sounds": [],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "L'application a besoin d'accéder à vos photos pour sauvegarder les images de prières.",
          "savePhotosPermission": "L'application a besoin d'accéder à vos photos pour sauvegarder les images de prières.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cffe1d78-5e76-42e6-9d07-68b58521f1d2"
      }
    },
    "owner": "iamfrancisco"
  }
}
